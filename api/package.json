        "@nestjs/passport": "^9.0.3",
        "@nestjs/platform-express": "^9.0.0",
        "@nestjs/schedule": "^2.2.0",
        "@nestjs/serve-static": "^3.0.1",
        "@nestjs/swagger": "^6.2.1",
        "@nestjs/typeorm": "^9.0.1",
        "@types/bcrypt": "^5.0.0",
        "app-root-path": "^3.1.0",
        "bcrypt": "^5.1.0",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cookie-parser": "^1.4.6",
        "cross-env": "^7.0.3",
        "csurf": "^1.11.0",
        "date-fns": "^2.29.3",
        "fs-extra": "^11.1.0",
        "helmet": "^6.0.1",
        "nest-winston": "^1.9.1",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.9.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^5.0.0",
        "rxjs": "^7.2.0",
        "sharp": "^0.31.3",
        "slugify": "^1.6.5",
